<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="mrp_production_form_inherit_starplastic_shift_buttons" model="ir.ui.view">
    <field name="name">mrp.production.form.inherit.starplastic.shift.buttons</field>
    <field name="model">mrp.production</field>
    <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
    <field name="arch" type="xml">
          <xpath expr="//div[@name='button_box']" position="inside">
        <field name="show_lock" invisible="1"/>
        <button type="object"
                name="action_view_shifts"
                class="oe_stat_button"
                icon="fa-calendar"
                string="Shifts"/>
      </xpath>
    </field>
  </record>
</odoo>